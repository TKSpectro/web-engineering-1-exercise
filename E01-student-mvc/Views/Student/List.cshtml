@model IEnumerable<Student>

<h2>@ViewBag.Title</h2>

<table class="table">
    <tr>
        <th>
            Firstname
        </th>
        <th>
            Lastname
        </th>
        <th>
            Student Number
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @item.Firstname
        </td>
        <td>
            @item.Lastname
        </td>
        <td>
            @item.StudentNumber
        </td>
        <td>
              @Html.ActionLink("Details", "Details", new { id=item.Id })
        </td>
    </tr>
}
</table>